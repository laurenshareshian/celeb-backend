CREATE TABLE profile (
  profile_id SERIAL UNIQUE,
  name varchar(45),
  age int,
  gender varchar(45),
  celeb_status varchar(45),
  bio varchar(45),
  fk_email_id int,
  FOREIGN KEY (fk_email_id) REFERENCES login (email_id));

INSERT INTO profile (name, age, gender, celeb_status, bio, fk_email_id) VALUES ('admin_name', 25, 'M', 'Really Famous', 'I read good.', 1);
